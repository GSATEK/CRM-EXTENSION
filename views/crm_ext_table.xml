<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="crm_table_tree" model="ir.ui.view">
        <field name="name">crm.table.tree</field>
        <field name="model">crm.table</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="crm_stage_ids"/>
                <field name="stage_count"/>
            </tree>
        </field>
    </record>

    <record id="crm_table_form" model="ir.ui.view">
        <field name="name">crm.table.form</field>
        <field name="model">crm.table</field>
        <field name="priority" eval="1"/>
        <field name="arch" type="xml">
            <form string="Table">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="description"/>
                            <field name="crm_stage_ids"/>
                            <field name="stage_count"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>

